<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.graduation">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.SelectFunctionActivity"
            android:label="@string/title_questions_and_answers"></activity>
        <provider
            android:name="cn.bmob.v3.util.BmobContentProvider"
            android:authorities="com.example.graduation.BmobContentProvider" />

        <activity android:name=".Activity.VerifyPhoneActivity" android:label="@string/title_questions_and_answers"/>
        <activity android:name=".Activity.SetPasswordActivity" android:label="@string/title_questions_and_answers"/>
        <activity android:name=".Activity.PostQuestionActivity" android:label="@string/title_questions_and_answers"/>
        <activity android:name=".Activity.AnswerQuestionActivity" android:label="@string/title_questions_and_answers"/>
        <activity android:name=".Activity.ShowAnswerListActivity" android:label="@string/title_questions_and_answers"/>
        <activity android:name=".Activity.ShowSearchQuestionListActivity" android:label="@string/title_questions_and_answers"/>
        <activity android:name=".Activity.MyFollowsActivity" android:label="@string/title_questions_and_answers"/>
        <activity android:name=".Activity.MyQuestionsActivity" android:label="@string/title_questions_and_answers"/>
        <activity android:name=".Activity.MySupportsActivity" android:label="@string/title_questions_and_answers"/>
        <activity android:name=".Activity.MyAnswersActivity" android:label="@string/title_questions_and_answers"/>
        <activity android:name=".Activity.SetPersonalTagsActivity" android:label="@string/title_questions_and_answers"/>
        <activity android:name=".Activity.LoginActivity" android:label="@string/title_questions_and_answers"/>
        <activity android:name=".Activity.ChooseLanguageActivity" android:label="@string/title_questions_and_answers"/>
        <activity android:name=".Activity.WelcomeActivity" android:label="@string/title_questions_and_answers">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>